@{
    ViewData["Title"] = "Sultan Sklep - Home Page";
}


<section class="hero">
    <h1>Sultan Sklep Mięsny</h1>
    <p>Najlepsza jakość mięsa prosto z natury</p>
    <a href="#" class="watch-btn"><i class="fa-solid fa-play"></i> Odtwórz</a>
</section>

<!-- Product Section -->
<section class="product-section">
    <div class="container text-center">
        <h2>Nasze najlepsze produkty mięsne</h2>
        <p>Wybierz spośród naszej starannie wyselekcjonowanej oferty najwyższej jakości jagnięciny i wołowiny</p>

        <!-- Carousel -->
        <div id="productCarousel" class="carousel slide" data-bs-ride="carousel">
            <!-- Carousel Items -->
            <div class="carousel-inner">
                <!-- Slide 1 -->
                <div class="carousel-item active">
                    <div class="row">
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 1">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 2">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 3">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 4">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                    </div>
                </div>
                <!-- Slide 2 -->
                <div class="carousel-item">
                    <div class="row">
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 5">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 6">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 7">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                        <div class="col-md-3">
                            <img src="~/assets/img/product-image.jpg" class="img-fluid rounded" alt="Product 8">
                            <div class="product-name mt-2">Kotlety jagnięce premium</div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Carousel Controls -->
            <button class="carousel-control-prev" type="button" data-bs-target="#productCarousel" data-bs-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Previous</span>
            </button>
            <button class="carousel-control-next" type="button" data-bs-target="#productCarousel" data-bs-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="visually-hidden">Next</span>
            </button>
        </div>
        <!-- Carousel End -->
    </div>
</section>


